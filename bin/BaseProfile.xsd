<?xml version="1.0" encoding="utf-8" ?>
<xs:schema xmlns="http://schemas.iz3d.com/baseprofile/2007"  attributeFormDefault="unqualified" elementFormDefault="qualified"
           targetNamespace="http://schemas.iz3d.com/baseprofile/2007" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="Profile.xsd" />
  <xs:element name="Profiles">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" name="Profile" type="Profile" />
      </xs:sequence>
      <xs:attribute name="Version" type="xs:unsignedByte" use="required" />
    </xs:complexType>
  </xs:element>
</xs:schema>